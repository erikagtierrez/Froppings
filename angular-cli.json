{
    "project": {
        "version": "1.0.0-beta.19-3",
        "name": "froppings-website"
    },
    "apps": [{
        "root": "src",
        "outDir": "dist",
        "assets": [
            "assets",
            "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.json",
        "prefix": "app",
        "mobile": false,
        "styles": [
            "styles.css",
            "assets/css/bootstrap/bootstrap.min.css",
            "assets/font-awesome/css/font-awesome.min.css",
            "assets/css/plugins/owl-carousel/owl.carousel.css",
            "assets/css/plugins/owl-carousel/owl.theme.css",
            "assets/css/plugins/owl-carousel/owl.transitions.css",
            "assets/css/plugins/magnific-popup.css",
            "assets/css/plugins/background.css",
            "assets/css/plugins/animate.css",
            "assets/css/resetricmeyer.css",
            "assets/css/vitality-tan.css"
        ],
        "scripts": [
            "../node_modules/jquery/dist/jquery.min.js",
            "../node_modules/materialize-css/dist/js/materialize.min.js",
            "assets/js/plugins/retina/retina.min.js",
            "assets/js/jquery.js",
            "assets/js/plugins/jquery.mixitup.js",
            "assets/js/bootstrap/bootstrap.min.js",
            "assets/js/plugins/jquery.easing.min.js",
            "assets/js/plugins/cbpAnimatedHeader.js",
            "assets/js/plugins/jquery.magnific-popup/jquery.magnific-popup.min.js",
            "assets/js/plugins/background/core.js",
            "assets/js/plugins/background/transition.js",
            "assets/js/plugins/background/background.js",
            "assets/js/plugins/wow/wow.min.js",
            "assets/js/vitality.js"
        ],
        "environments": {
            "source": "environments/environment.ts",
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
        }
    }],
    "addons": [],
    "packages": [],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "css",
        "prefixInterfaces": false,
        "inline": {
            "style": false,
            "template": false
        },
        "spec": {
            "class": false,
            "component": true,
            "directive": true,
            "module": false,
            "pipe": true,
            "service": true
        }
    }
}